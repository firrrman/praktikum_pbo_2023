package Tugas;

import java.util.Scanner;

public class Tugas1{
	public int angka1;
	public int angka2;
	
	public int getAngka1() {
		return angka1;
	}
	public void setAngka1(int angka1) {
		this.angka1 = angka1;
	}
	public int getAngka2() {
		return angka2;
	}
	public void setAngka2(int angka2) {
		this.angka2 = angka2;
	}
	
	public int tambah() {
		return angka1 + angka2;
	}
	public int kurang() {
		return angka1 - angka2;
	}
	public int kali() {
		return angka1 * angka2;
	}
	public int bagi() {
		return angka1 / angka2;
	}
}