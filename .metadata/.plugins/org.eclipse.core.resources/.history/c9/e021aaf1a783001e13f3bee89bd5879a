package tugas;

public class Coba1{
	private int angka1;
	private int angka2;
	
	public int getAngka1() {
		return angka1;
	}
	public void setAngka1(int angka1) {
		this.angka1 = angka1;
	}
	public int getAngka2() {
		return angka2;
	}
	public void setAngka2(int angka2) {
		this.angka2 = angka2;
	}
	
	public int tambah() {
		this angka1 + angka2;
	}
	public int kurang() {
		this angka1 - angka2;
	}
	public int kali() {
		this angka1 * angka2;
	}
	public int bagi() {
		this angka1 / angka2;
	}
	
	
}